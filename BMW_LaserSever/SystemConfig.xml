<?xml version="1.0" encoding="utf-8" ?>
<Luxoom>
  <Laser>
    <!-- Laser Scan Information -->
    <Server IP="192.168.0.245" Port="2112" />
    
    <Setting Direction="LeftToRight" TimerInterval="25" BlobSize="5">

      <Config Name="LeftToRight">
        <!-- Scaner Information -->
        <Scan Start="0" End="180" />

        <!-- Data we need -->
        <Data diameter="3000" Start="0" End="180" Correction="0"/>
      </Config>

      <Config Name="RightToLeft">
        <!-- Scaner Information -->
        <Scan Start="0" End="180" />
        
        <!-- Data we need -->
        <Data diameter="3000" Start="0" End="180" Correction="0.05"/>
      </Config>
      
    </Setting>
  </Laser>
</Luxoom>
